	<queryString>
		<![CDATA[SELECT
            p.employee_id                                            AS EmployeeNumber,
            CONCAT(e.first_name, ' ', e.last_name)                   AS EmployeeFullName,
            p.position                                               AS Position,
            p.department                                             AS Department,
            SUM(p.gross_salary)                                      AS GrossIncome,
            gi.sss_number                                            AS SocialSecurityNumber,
            SUM(p.total_deductions)                                  AS SocialSecurityContribution,
            gi.pagibig_number                                        AS PagibigNumber,
            SUM(p.total_deductions)                                  AS PagibigContribution,
            gi.philhealth_number                                     AS PhilHealthNumber,
            SUM(p.total_deductions)                                  AS PhilHealthContribution,
            gi.tax_identification_number                             AS TIN,
            SUM(p.total_deductions)                                  AS WithholdingTax
          FROM
            payslip p
            INNER JOIN employee_personal_information e
              ON p.employee_id = e.employee_id
            INNER JOIN employee_government_information gi
              ON e.employee_id = gi.employee_id
          WHERE
            p.generated_date BETWEEN $P{P_START_DATE} AND $P{P_END_DATE}
          GROUP BY
            p.employee_id,
            e.first_name,
            e.last_name,
            p.position,
            p.department,
            gi.sss_number,
            gi.pagibig_number,
            gi.philhealth_number,
            gi.tax_identification_number
          ORDER BY
            e.last_name,
            e.first_name]]>
	</queryString>
