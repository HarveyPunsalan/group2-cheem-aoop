	<parameter name="P_PAYSLIP_ID" class="java.lang.Integer" isForPrompting="false"/>
	<queryString>
  <![CDATA[
    SELECT
      p.payslip_id,
      p.employee_id,

      -- build the full name from first_name + last_name
      CONCAT(u.last_name, ', ', u.first_name) AS employee_name,

      p.position,
      p.department,
      p.monthly_rate,
      p.daily_rate,
      p.days_worked,
      p.overtime,
      p.gross_salary,
      p.rice_subsidy,
      p.phone_allowance,
      p.clothing_allowance,
      p.sss,
      p.pagibig,
      p.philhealth,
      p.tin,
      p.total_allowance,
      p.total_deductions,
      p.net_salary,
      p.generated_date,
      p.period_start,
      p.period_end
    FROM
      payslip p
      LEFT JOIN system_user u
        ON p.employee_id = u.user_id
    WHERE
      p.payslip_id = $P{P_PAYSLIP_ID}
  ]]>
</queryString>

